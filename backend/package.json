        "discord.js": "^14.19.3",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "express-slow-down": "^3.0.0",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "prom-client": "^15.1.3",
