{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon --watch src --exec ts-node src/index.ts",
        "dev:api": "nodemon --watch src --exec ts-node --files src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest __tests__/unit",
        "test:integration": "jest __tests__/integration",
        "test:e2e": "jest __tests__/e2e",
        "build": "tsc",
        "type-check": "tsc --noEmit",
        "sentry:sourcemaps": "./scripts/upload-sourcemaps.sh",
        "prisma:validate": "prisma validate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate deploy",
        "prisma:migrate:dev": "prisma migrate dev",
        "prisma:seed": "ts-node prisma/seed.ts",
        "db:migrate": "ts-node ../scripts/migrate-to-postgres.ts",
        "db:migrate:dry": "DRY_RUN=true ts-node ../scripts/migrate-to-postgres.ts",
        "db:backup": "../scripts/backup.sh",
        "db:restore": "../scripts/restore.sh",
        "db:maintenance": "../scripts/maintenance.sh",
        "db:fulltext": "../scripts/setup-fulltext-search.sh",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@prisma/client": "^6.18.0",
